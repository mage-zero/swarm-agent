{
  "name": "swarm-agent",
  "version": "0.4.61",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "build:bundle": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/swarm-agent.cjs",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.1",
    "hono": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^2.1.9",
    "esbuild": "^0.25.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
